<script type="text/javascript">
//Cuando el documento esté listo realizamos las consultas
  $(document).ready(function(){
    var fechaInicio = $("#datepicker").val();
    var fechaFin = $("#datepicker2").val();
    intervalo(fechaInicio, fechaFin);
  });

//Llenar los input de las Fechas para que no empiecen vacios
  var today = new Date();
  var dd = today.getDate();
  var mm = today.getMonth()+1; //January is 0!
  var yyyy = today.getFullYear();
  if(dd<10) {
      dd = '0'+dd
  } 
  if(mm<10) {
      mm = '0'+mm
  } 
  today = mm + '/' + dd + '/' + yyyy;

  $('#datepicker2').val(today);

  var init = new Date();
  init.setMonth(init.getMonth() - 1);
  var dd = init.getDate();
  var mm = init.getMonth()+1; //January is 0!
  var yyyy = init.getFullYear();
  if(dd<10) {
      dd = '0'+dd
  } 
  if(mm<10) {
      mm = '0'+mm
  } 
  init = mm + '/' + dd + '/' + yyyy;

  $('#datepicker').val(init);

  
  //Date picker Inicio
  $('#datepicker').datepicker({
    autoclose: true,
  })
  //Date picker Termino
  $('#datepicker2').datepicker({
    autoclose: true
  })

  $("#datepicker").on("change", function(){
    var fechaInicio = $("#datepicker").val();
    var fechaFin = $("#datepicker2").val();
    intervalo(fechaInicio, fechaFin);
  });

  $("#datepicker2").on("change", function(){
    var fechaInicio = $("#datepicker").val();
    var fechaFin = $("#datepicker2").val();
    intervalo(fechaInicio, fechaFin);
  });


  //Esta función hace asincronamente las diferentes consultas a la API de SBIF y llena la página para que sea dinámica
  function intervalo(fechaInicio, fechaFin){
    var dataDolar = [];
    var dataUF = [];
    var dataTMC = [];
    var dataTMC26 = [];
    var dataTMC25 = [];
    var dataTMC45 = [];
    var dataTMC44 = [];
    var dataTMC35 = [];
    var dataTMC43 = [];
    var dataTMC21 = [];
    var dataTMC24 = [];
    var dataTMC22 = [];
    var dataTMC41 = [];
    var dataTMC42 = [];
    var dataTMC34 = [];

    var dI = new Date(fechaInicio);
    var dF = new Date(fechaFin);
    var dayI = dI.getDate();
    var monthI = dI.getMonth();
    var yearI = dI.getFullYear();
    var dayF = dF.getDate();
    var monthF = dF.getMonth();
    var yearF = dF.getFullYear();
    
    $("#line-chart-dolar").html("");
    $("#line-chart-uf").html("");
    $("#line-chart-tmc").html("");
    $("#data-dolar").html("");
    $("#data-uf").html("");
    $("#data-tmc").html("");
    

    $.ajax({
      url: 'http://api.sbif.cl/api-sbifv3/recursos_api/dolar/posteriores/'+(yearI-1)+'?apikey=b8124793da9ca97350a3be40583dd49e1c07e51c&formato=json',
      type:  'get',
      success:  function (responseDolar) {
        data = responseDolar['Dolares'];
        maxDolar = 0;
        minDolar = 10000;
        promDolar = 0;
        iD=0;

        $.each(data, function(index, value){
          fecha = new Date(value['Fecha']);
          //Este extracto de código cambia el formato del JSON para poder manejar los valores como Float
          valorD = value.Valor;
          valorD = valorD.replace(".", "");
          valorD = valorD.replace(",", ".");
          value.Valor = valorD;
          if(fecha >= dI && fecha <= dF){
            console.log("HOLAFECHA");
            if (parseFloat(value.Valor) > maxDolar) {maxDolar = parseFloat(value.Valor)};
            if (parseFloat(value.Valor) < minDolar) {minDolar = parseFloat(value.Valor)};
            promDolar+=parseFloat(value.Valor);
            iD++;
            dataDolar.push(value);
            $("#data-dolar").append("<dt>" + value['Fecha'] + "</dt><dd>$"+ parseFloat(value.Valor) +"</dd>");
          }
        });
        $("#dolar-max").append("$"+maxDolar);
        $("#dolar-min").append("$"+minDolar);
        $("#dolar-prom").append("$"+parseFloat(promDolar/iD));
        console.log(dataDolar);
        // LINE CHART
        var line_dolar = new Morris.Line({
          element: 'line-chart-dolar',
          resize: true,
          data: dataDolar,
          xkey: 'Fecha',
          ykeys: ['Valor'],
          labels: ['Valor'],
          lineColors: ['#3c8dbc'],
          hideHover: 'auto'
        });
