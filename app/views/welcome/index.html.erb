            var line_uf = new Morris.Line({
              element: 'line-chart-uf',
              resize: true,
              data: dataUF,
              xkey: 'Fecha',
              ykeys: ['Valor'],
              labels: ['Valor'],
              lineColors: ['#3c8dbc'],
              hideHover: 'auto'
            });
