          url: 'http://api.sbif.cl/api-sbifv3/recursos_api/uf/posteriores/'+(yearI-1)+'?apikey=b8124793da9ca97350a3be40583dd49e1c07e51c&formato=json',
          type:  'get',
          success:  function (responseUF) {
            data = responseUF['UFs'];
            maxUF = 0;
            minUF = 10000000;
            promUF = 0;
            iUF=0;

            $.each(data, function(index, value){
              fecha = new Date(value['Fecha']);
              //Este extracto de código cambia el formato del JSON para poder manejar los valores como Float
              valorUF = value.Valor;
              valorUF = valorUF.replace(".", "");
              valorUF = valorUF.replace(",", ".");
              value.Valor = valorUF;
              if(fecha >= dI && fecha <= dF){
                if (parseFloat(value.Valor) > maxUF) {maxUF = parseFloat(value.Valor)};
                if (parseFloat(value.Valor) < minUF) {minUF = parseFloat(value.Valor)};
                promUF+=parseFloat(value.Valor);
                iUF++;
                dataUF.push(value);
                $("#data-uf").append("<dt>" + value['Fecha'] + "</dt><dd>$"+ parseFloat(value.Valor) +"</dd>");
              }
            });
            $("#uf-max").append("$"+maxUF);
            $("#uf-min").append("$"+minUF);
            $("#uf-prom").append("$"+parseFloat(promUF/iUF));
            console.log(dataUF);
